<codeline><highlight class="normal">template<sp/>&lt;typename<sp/>T,<sp/>typename<sp/>IndexT&gt;</highlight></codeline>
<codeline><highlight class="normal">struct<sp/>TensorInfo<sp/>{</highlight></codeline>
